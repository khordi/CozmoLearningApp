#----------------------------------------------------------------------------------------
<MainWindow>:
    name: "main"

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'cozmo.png'

    GridLayout:
        cols: 1
        spacing: 10

        Label:
            color: 1,1,1,1
            outline_width:1
            outline_color: 0,0,0,1
            bold: True
            text: "Please choose a topic to learn"
            halign: 'center'
            size_hint: None, None
            size: 600,50
            font_size: 25

        GridLayout:
            cols: 3
            spacing:2
            padding:2

            Button:
                background_normal: ''
                background_color: 0.90, 0.4, 0.1, 0.8
                size_hint: None, None
                size: 200, 200
                text: "Algebra"
                font_size: 30
                color: 1,1,1,1
                outline_width:1
                outline_color: 0,0,0,1
                
                on_release: 
                    app.root.current = "algebrawindow"
                    root.manager.transition.direction = "left"
                    #root.WhatisAlgebra()

            Button:
                background_normal: ''
                background_color: 0.90, 0.4, 0.1, 0.8               
                size_hint: None, None
                size: 200, 200 
                text: "Geometry"
                font_size: 30
                color: 1,1,1,1
                outline_width:1
                outline_color: 0,0,0,1
                on_release: 
                    app.root.current = "geometrywindow"
                    root.manager.transition.direction = "left"
                    
            Button:
                background_normal: ''
                background_color: 0.90, 0.4, 0.1, 0.8
                id:trig
                size_hint: None, None
                size: 200, 200 
                text: "Trigonometry"
                font_size: 30
                color: 1,1,1,1
                outline_width:1
                outline_color: 0,0,0,1
                on_release: 
                    app.root.current = "trigwindow"
                    root.manager.transition.direction = "left"

            FloatLayout:
                cols:2
                spacing: 300
                
                #Button:
                #    size_hint: None, None
                #    size: 200, 200 
                #    text: "      Student \n Leaderboards"
                #    font_size: 30

                #    on_release: 
                #        app.root.current = "trigwindow"
                #        root.manager.transition.direction = "left"

                Button:
                    background_normal: ''
                    background_color: 0.90, 0.4, 0.1, 0.8
                    size_hint: None, None
                    size: 400, 100 
                    text: "Help"
                    font_size: 30
                    pos_hint: {"x":0.5, "top":0.6}
                    on_release: root.btn()
