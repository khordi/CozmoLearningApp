<AlegbraWindow>:
    name: "algebrawindow"

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'cozmo.png'

    GridLayout:
        cols: 1
        spacing: 10

        Label:
            color: 0,0,0,1
            bold: True
            text: "Algebra"
            halign: 'center'
            size_hint: None, None
            size: 600,50
            

    GridLayout:
        cols: 1
        padding: 100,60
        #spacing: 50, 20

        Button:
            size_hint: None, None
            size: 400, 100         
            text: "Tutorial"
            on_release:  
                app.root.current = "algebratutorial"
                root.manager.transition.direction = "left"
        
        GridLayout:
            cols: 1
            padding: 0,40
            Button:
                size_hint: None, None
                size: 400, 100         
                text: "Questions and Answers"
                on_release:
                    root.titleSection()
                    app.root.current = "algebra1"
                    root.manager.transition.direction = "left"

        GridLayout:
            cols: 1
            padding: 100,-20
            Button:
                size_hint: None, None
                size: 200, 100         
                text: "Back"
                bold: True
                text_size: 130,0
                font_size: 30
                on_release: 
                    app.root.current = "main"
                    root.manager.transition.direction = "right"
                Image:
                    source: "./backarrow.png"
                    y: self.parent.y 
                    x: self.parent.center_x
