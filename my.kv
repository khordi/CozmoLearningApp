#algebra questions
<Algebra1>:

    name : "algebra1"
    answer: answer

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'cozmo.png'

    FloatLayout:
        cols:1

        FloatLayout:
            size: root.width, root.height/2

            Label:
                text: "Question 1"
                size_hint: 0.8, 0.2
                color: 0,0,0,1
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8}
                text: "Answer: "
                font_size: (root.width**2 + root.height**2) / 14**4
                color: 0,0,0,1
            TextInput:
                pos_hint: {"x":0.5, "top":0.8}
                size_hint: 0.4, 0.12
                id: answer
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4
                color: 0,0,0,1
            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.65}
                text: "Expression: "
                font_size: (root.width**2 + root.height**2) / 14**4
                color: 0,0,0,1
            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0.33, "top":0.65}
                text: "10x + 4y + b"
                font_size: (root.width**2 + root.height**2) / 14**4
                color: 0,0,0,1
        Button:
            pos_hint:{"x":0.2,"y":0.3}
            size_hint: 0.6, 0.15
            text: "Submit"
            font_size: (root.width**2 + root.height**2) / 14**4
            on_release:
                root.submit()
                root.manager.transition.direction = "left"
                app.root.current = "algebra2"
                root.question()

        Button:
            pos_hint:{"x":0.35,"y":0.10}
            size_hint: 0.3, 0.15
            text: "Repeat Question"
            font_size: (root.width**2 + root.height**2) / 14**4
            
            on_release:
                root.requestion()
