<AlgebraTutorial>:

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'cozmo.png'

    name: "algebratutorial"

    GridLayout:
        spacing: 2
        padding: 2.5

        cols: 3

        Button:
            id: expression_button
            size_hint: None, None
            size: 200, 200
            font_size:30
            color: 1,0.5,0,1        
            text: "Expressions \n        and \n   Variables"
            on_release: 
               # app.root.current = "mainalgebra"
               # root.manager.transition.direction = "right"
                root.checkSection()
                root.expression_variables()
                
        Button:
            id: button_one
            size_hint: None, None
            size: 200, 200
            font_size:30
            color: 1,0.5,0,1
            text: "Substitution \n        and \n   Evaluating"
            on_release: 
                app.root.current = "main"
                root.manager.transition.direction = "right"
        Button:
            size_hint: None, None
            size: 200, 200
            font_size:30
            color: 1,0.5,0,1
            text_size: self.size     
            halign: 'center'    
            text: "Evaluating \n Expressions \n and \n Word \n Problems"
            on_release: 
                app.root.current = "main"
                root.manager.transition.direction = "right"
        Button:
            size_hint: None, None
            size: 200, 200
            font_size:30
            color: 1,0.5,0,1  
            text: " Combining \n Like Terms"
            on_release: 
                app.root.current = "main"
                root.manager.transition.direction = "right"
        Button:
            size_hint: None, None
            size: 200, 200
            font_size:30
            color: 1,0.5,0,1     
            text: "  Algebraic \n Equations"
            on_release: 
                app.root.current = "main"
                root.manager.transition.direction = "right"
        Button:
            size_hint: None, None
            size: 200, 200   
            text: "Algebraic \n Equations"
            on_release: 
                app.root.current = "main"
                root.manager.transition.direction = "right"


